#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main()
{
    int t;
    cin >> t;

    vector<bool> ans;
    string name = "Timur";

    int len;
    int cou = 0;
    string s;

    for (int i = 0; i < t; i++)
    {
        cin >> len;
        cin >> s;
        if (len != 5) ans.push_back(0);
        else
        {
            for (int j = 0; j < 5; j++)
            {
                for (int c = 0; c < 5; c++)
                {
                    if (s[j] == name[c]) cou++;
                }
            }
            if (cou != 5) ans.push_back(0);
            else ans.push_back(1);
            cou = 0;
        }
    }

    for (int i = 0; i < t; i++)
    {
        if (ans[i] == 0) cout << "NO" << endl;
        else cout << "YES" << endl;
    }
    system("pause");
}
